{
  "name": "angular-starter",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "bundle:analyze": "source-map-explorer dist/**/*.js",
    "bundle:build": "ng build --prod --source-map",
    "clean": "concurrently \"rimraf coverage\" \"rimraf dist\" \"rimraf npm-debug.log*\" \"npm cache verify\" \"npm prune\"",
    "lint": "npm run lint:ts && npm run lint:scss && npm run lint:html",
    "lint:html": "js-beautify src/**/*.html -r",
    "lint:ts": "ng lint",
    "lint:scss": "stylelint src/**/*.scss --syntax scss --fix",
    "start": "ng serve",
    "start:build": "ws --spa index.html --directory dist --hostname localhost --port 1337",
    "test": "ng test --verbose"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.8",
    "@angular/common": "13.3.8",
    "@angular/compiler": "13.3.8",
    "@angular/core": "13.3.8",
    "@angular/forms": "13.3.8",
    "@angular/platform-browser": "13.3.8",
    "@angular/platform-browser-dynamic": "13.3.8",
    "@angular/router": "13.3.8",
    "bulma": "0.9.4",
    "bulma-slider": "2.0.5",
    "bulma-switch": "2.0.4",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-builders/jest": "13.0.4",
    "@angular-devkit/build-angular": "13.3.5",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "13.3.5",
    "@angular/compiler-cli": "13.3.8",
    "@commitlint/cli": "16.3.0",
    "@commitlint/config-conventional": "16.2.4",
    "@ngneat/spectator": "10.0.1",
    "@types/jest": "27.5.1",
    "@types/jest-when": "3.5.0",
    "@types/node": "17.0.33",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "concurrently": "7.1.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.2.1",
    "eslint-plugin-no-use-extend-native": "0.5.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "42.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.1",
    "jasmine-marbles": "0.9.2",
    "jest": "28.1.0",
    "jest-extended": "2.0.0",
    "jest-junit": "13.2.0",
    "jest-preset-angular": "12.0.0",
    "jest-when": "3.5.1",
    "js-beautify": "1.14.3",
    "lint-staged": "12.4.1",
    "local-web-server": "5.2.0",
    "mockdate": "3.0.5",
    "ng-mocks": "13.5.1",
    "prettier": "2.6.2",
    "prettier-eslint": "14.0.3",
    "source-map-explorer": "2.5.2",
    "stylelint": "14.8.2",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard": "25.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.5.0",
    "stylelint-no-unsupported-browser-features": "5.0.3",
    "stylelint-order": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "stylelint-scss": "4.2.0",
    "stylelint-use-nesting": "3.0.0",
    "stylelint-z-index-value-constraint": "1.2.0",
    "typescript": "4.6.4"
  }
}
